X_USERNAME=
X_PASSWORD=

# Windows:
# CHROME_PROFILE_DIR=C:\Users\<user>\AppData\Local\Google\Chrome\User Data\Default
# CHROME_PATH=C:\Program Files\Google\Chrome\Application\chrome.exe

# macOS:
# CHROME_PROFILE_DIR=~/Library/Application Support/Google/Chrome/Default
# CHROME_PATH=/Applications/Google Chrome.app/Contents/MacOS/Google Chrome

# Linux:
# CHROME_PROFILE_DIR=~/.config/google-chrome/Default
# CHROME_PATH=/usr/bin/google-chrome

# Leave empty to use Playwright's built-in Chromium
CHROME_PROFILE_DIR=
CHROME_PATH=

HEADLESS=false
CHECK_INTERVAL_SECONDS=15
MAX_RETRIES=1
